<?xml version="1.0" encoding="utf-8"?>
<data>

	<definition id="btn" width="90" height="20" />

	<button id="butt_0" label="Race" use_def="btn">
		<anchor x="3%" x-flush="left" />
		<param type="int" value="0" />
	</button>
	<button id="butt_1" label="Options" use_def="btn">
		<anchor x="3%" x-flush="left" />
		<param type="int" value="1" />
	</button>
	<button id="butt_2" label="Quit" use_def="btn">
		<anchor x="3%" x-flush="left" />
		<param type="int" value="2" />
	</button>

	<align axis="vertical">
		<objects value="butt_0,butt_1,butt_2" />
		<bounds top="20%" bottom="75%" />
	</align>

	<text id="game_title" x="3%" y="3%" align="left" width="20%" color="0xFFFFFF" text="Tiny Busters" />

</data>